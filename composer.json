{
    "name": "acquia/drupal-recommended-project",
    "description": "Arvest Bank Acquia-compatible Drupal application based on the Drupal Recommended Project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "acquia/blt": "12.4",
        "acquia/blt-simplesamlphp": "dev-master",
        "acquia/blt-site-studio": "dev-develop",
        "acquia/cohesion": "6.5.1",
        "acquia/cohesion-theme": "6.5.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_lift": "~4.0",
        "drupal/acquiadam_asset_import": "^1.1",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/chosen": "^3.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^2.0@beta",
        "drupal/config_split_ignore": "^1.9",
        "drupal/context": "^4.0@beta",
        "drupal/context_entity_field": "1.x-dev@dev",
        "drupal/cookiepro": "^2.0",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "9.2.0",
        "drupal/critical_css": "^1.17",
        "drupal/ctools": "^3.6",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.1",
        "drupal/extlink": "^1.6",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/geolocation": "^3.5",
        "drupal/gtm": "^1.6",
        "drupal/honeypot": "^2.0",
        "drupal/http_cache_control": "^2.0",
        "drupal/iframe": "^2.8",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/link_attributes": "^1.11",
        "drupal/linkit": "5.0.0-beta12",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_acquiadam": "^1.47",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menu_block": "^1.6",
        "drupal/menu_item_extras": "^2.14",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.2",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/mysql56": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/pathauto_menu_link": "1.x-dev",
        "drupal/qa_accounts": "1.x-dev@dev",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/responsive_tables_filter": "^1.7",
        "drupal/scheduler": "^1.3",
        "drupal/search_api": "^1.19",
        "drupal/search_api_attachments": "^1.0@beta",
        "drupal/seckit": "^2.0",
        "drupal/shield": "^1.4",
        "drupal/simple_instagram_feed": "^3.9",
        "drupal/simple_sitemap": "^3.8",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/sitemap": "^2.0@beta",
        "drupal/taxonomy_menu_ui": "^1.4",
        "drupal/views_autosubmit": "^1.4",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/webform": "^6.0",
        "drupal/workbench": "^1.3",
        "drupal/yoast_seo": "^1.7",
        "drupal/youtube": "^1.2",
        "drush/drush": "^10.4",
        "econea/nusoap": "^0.9.11",
        "oomphinc/composer-installers-extender": "^2.0",
        "smalot/pdfparser": "^0.18.2",
        "spatie/array-to-xml": "^2.15",
        "sunra/php-simple-html-dom-parser": "^1.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                },
                "[web-root]/.htaccess": {
                    "append": "patches/htaccessappend.txt"
                }
            },
            "gitignore": true
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "FinishResponseSubscriber: Need warning/error when headers exceed 16k": "https://www.drupal.org/files/issues/2021-02-08/finish-response-subscriber--need-warning-error-when-headers-exceed-16k-2844620-34.patch"
            },
            "drupal/field_group": {
                "Conditional Fields support": "https://www.drupal.org/files/issues/2019-12-10/field_group-support_conditional_fields-3099051-6.patch"
            },
            "acquia/cohesion": {
                "Allow master templates to use to be defined at route level.\n": "patches/cohesion-master-template-by-route.patch"
            },
            "drupal/simplesamlphp_auth": {
                "fix roles": "https://www.drupal.org/files/issues/2020-07-13/simplesamlphp_auth-role_assignment-2894945-11.patch"
            },
            "drupal/media_acquiadam": {
                "Module only themed to work with SEVEN": "https://www.drupal.org/files/issues/2021-04-27/module-theming-3210805-1_0.patch",
                "Acquia DAM auth breaks when SameSite is enforced": "https://www.drupal.org/files/issues/2021-06-08/3217950-4.patch"
            },
            "drupal/diff": {
                "Filter Revision Overview By Date": "https://www.drupal.org/files/issues/2021-05-06/filter-revision-overview-by-date-3212653-1.patch"
            },
            "sunra/php-simple-html-dom-parser": {
                "Update simple_html_dom.php": "https://patch-diff.githubusercontent.com/raw/sunra/php-simple-html-dom-parser/pull/78.patch"
            },
            "drupal/feeds": {
                "Undefined Index: allowed_extensions AJAX error when creating feed types.": "https://www.drupal.org/files/issues/2019-02-06/3031019-undefined-index-ajax-2.patch"
            },
            "drupal/redirect": {
                "Feeds: redirects_source field target class.": "https://www.drupal.org/files/issues/2019-04-09/redirect-feeds-target-3046873-1-8.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-site-studio": {
            "type": "vcs",
            "url": "https://github.com/davidtrainer/blt-site-studio.git"
        },
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "acquia/blt-drupal-vm": "dev-master",
        "drupal/console": "~1.0"
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-saml.patch"
        ]
    }
}
