        "econea/nusoap": "^0.9.11",
        "oomphinc/composer-installers-extender": "^2.0",
        "smalot/pdfparser": "^0.18.2",
        "spatie/array-to-xml": "^2.15",
        "sunra/php-simple-html-dom-parser": "^1.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                },
                "[web-root]/.htaccess": false
            },
            "gitignore": true
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "FinishResponseSubscriber: Need warning/error when headers exceed 16k": "https://www.drupal.org/files/issues/2021-02-08/finish-response-subscriber--need-warning-error-when-headers-exceed-16k-2844620-34.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush\n": "https://www.drupal.org/files/issues/2021-01-04/config_ignore_2857247-61.patch"
            },
            "drupal/scheduler_content_moderation_integration": {
                "Hide scheduling state options if there are no valid transitions\n": "https://www.drupal.org/files/issues/2020-07-16/2798689-hide-scheduling-options-if-no-valid-transitions-20.patch"
            },
            "acquia/cohesion": {
                "Allow master templates to use to be defined at route level.\n": "patches/cohesion-master-template-by-route.patch",
                "Stop Site Studio from going into maintenance mode": "patches/site-studio-no-maintenance-mode-patch.diff"
            },
            "drupal/simplesamlphp_auth": {
                "fix roles": "https://www.drupal.org/files/issues/2020-07-13/simplesamlphp_auth-role_assignment-2894945-11.patch"
            },
            "drupal/diff": {
                "Filter Revision Overview By Date": "https://www.drupal.org/files/issues/2021-05-06/filter-revision-overview-by-date-3212653-1.patch"
            },
            "sunra/php-simple-html-dom-parser": {
                "Update simple_html_dom.php": "https://patch-diff.githubusercontent.com/raw/sunra/php-simple-html-dom-parser/pull/78.patch"
            },
            "drupal/feeds": {
                "Undefined Index: allowed_extensions AJAX error when creating feed types.": "https://www.drupal.org/files/issues/2019-02-06/3031019-undefined-index-ajax-2.patch"
            },
            "drupal/redirect": {
                "Feeds: redirects_source field target class.": "https://www.drupal.org/files/issues/2019-04-09/redirect-feeds-target-3046873-1-8.patch"
            },
            "drupal/purge_purger_http": {
                "scheme is https not work": "https://www.drupal.org/files/issues/2018-11-29/2892552-verify-option-scheme-update-6.patch"
            },
            "drupal/extlink": {
                "fix double warn": "patches/extlink-stop-propagation.patch"
            },
            "drupal/condition_query": {
                "Add support for a wildcard parameter and a case sensitive option": "https://www.drupal.org/files/issues/2021-06-04/2986466-25.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-site-studio": {
            "type": "vcs",
            "url": "https://github.com/davidtrainer/blt-site-studio.git"
        },
        "site-studio-webform": {
            "type": "vcs",
            "url": "https://github.com/pavlosdan/site_studio_webform.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "acquia/blt-drupal-vm": "dev-master",
        "drupal/console": "~1.0"
    }
}
