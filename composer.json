{
    "name": "acquia/drupal-recommended-project",
    "description": "Arvest Bank Acquia-compatible Drupal application based on the Drupal Recommended Project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "acquia/blt": "12.4",
        "acquia/blt-simplesamlphp": "dev-master",
        "acquia/blt-site-studio": "dev-develop",
        "acquia/cohesion": "6.4.3",
        "acquia/cohesion-theme": "6.4.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^3.0",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/chosen": "^3.0",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^2.0@beta",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "9.1.5",
        "drupal/critical_css": "^1.17",
        "drupal/devel": "^4.1",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.1",
        "drupal/external_link_popup": "^1.8",
        "drupal/field_group": "^3.1",
        "drupal/gtm": "^1.6",
        "drupal/honeypot": "^2.0",
        "drupal/http_cache_control": "^2.0",
        "drupal/iframe": "^2.8",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/link_attributes": "^1.11",
        "drupal/linkit": "5.0.0-beta12",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_acquiadam": "^1.47",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menu_block": "^1.6",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.2",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/mysql56": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/pathauto_menu_link": "1.x-dev",
        "drupal/qa_accounts": "1.x-dev@dev",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/scheduler": "^1.3",
        "drupal/search404": "^2.0",
        "drupal/seckit": "^2.0",
        "drupal/shield": "^1.4",
        "drupal/simple_instagram_feed": "^3.9",
        "drupal/simple_sitemap": "^3.8",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/sitemap": "^2.0@beta",
        "drupal/webform": "^6.0",
        "drupal/workbench": "^1.3",
        "drupal/yoast_seo": "^1.7",
        "drupal/youtube": "^1.2",
        "drush/drush": "^10.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            },
            "gitignore": true
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/conditional_fields": {
                "Notice: Trying to access array offset on value of type null\n": "https://www.drupal.org/files/issues/2021-01-21/3191984-3-notice-trying-to-access-offset-on-value-of-type-null.patch"
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-site-studio": {
            "type": "vcs",
            "url": "https://github.com/davidtrainer/blt-site-studio.git"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "acquia/blt-drupal-vm": "dev-master",
        "drupal/console": "~1.0"
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-saml.patch"
        ]
    }
}
