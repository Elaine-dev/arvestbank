        "drupal/config_split": "^2.0@beta",
        "drupal/config_split_ignore": "^1.9",
        "drupal/context": "^4.0@beta",
        "drupal/context_entity_field": "1.x-dev@dev",
        "drupal/cookiepro": "^2.0",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "9.2.4",
        "drupal/critical_css": "^1.17",
        "drupal/ctools": "^3.6",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.0",
        "drupal/domain": "^1.0@beta",
        "drupal/domain_path_redirect": "^1.3",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.1",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^1.6",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/geolocation": "^3.5",
        "drupal/gtm": "^1.6",
        "drupal/honeypot": "^2.0",
        "drupal/http_cache_control": "^2.0",
        "drupal/iframe": "^2.8",
        "drupal/imageapi_optimize": "^4.0@beta",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/link_attributes": "^1.11",
        "drupal/linkit": "5.0.0-beta12",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_acquiadam": "^1.48",
        "drupal/media_video_micromodal": "1.x-dev@dev",
        "drupal/memcache": "2.x-dev@dev",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menu_block": "^1.6",
        "drupal/menu_item_extras": "^2.14",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.2",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/moderation_sidebar": "^1.4",
        "drupal/mysql56": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
